{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "sportyspots",
      "image": "359513016071.dkr.ecr.eu-central-1.amazonaws.com/sportyspots:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "environment": [
        {
          "name": "Container",
          "value": "PHP"
        }
      ],
    }
  ]
}
